<root>
    <AnalogueModels>
        <AnalogueModel type="SimplePathlossModel" thresholding="true">
            <parameter name="alpha" type="double" value="2.0"/>
        </AnalogueModel>
      	<AnalogueModel type="SimpleObstacleShadowing" thresholding="true">
            <obstacles>
                <type id="building" db-per-cut="0" db-per-meter="0" />
                
                
                
                <type id="water" db-per-cut="0" db-per-meter="0" />
                <type id="natural" db-per-cut="0" db-per-meter="0" />
                <type id="water" db-per-cut="0" db-per-meter="0" />
                <type id="forest" db-per-cut="0" db-per-meter="0" />
                <type id="land" db-per-cut="0" db-per-meter="0" />
                
                <!--<type id="landuse" db-per-cut="0" db-per-meter="0" />-->
                <type id="forest" db-per-cut="0" db-per-meter="0" />
                <type id="park" db-per-cut="0" db-per-meter="0" />
                <type id="residential" db-per-cut="0" db-per-meter="0" />
                <type id="commercial" db-per-cut="0" db-per-meter="0" />
                <type id="industrial" db-per-cut="0" db-per-meter="0" />
                <type id="military" db-per-cut="0" db-per-meter="0" />
                <type id="farm" db-per-cut="0" db-per-meter="0" />
                
                <type id="tourism" db-per-cut="0" db-per-meter="0" />
                <type id="military" db-per-cut="0" db-per-meter="0" />
                <type id="sport" db-per-cut="0" db-per-meter="0" />
                <type id="leisure" db-per-cut="0" db-per-meter="0" />
                <type id="aeroway" db-per-cut="0" db-per-meter="0" />
                <type id="aerialway" db-per-cut="0" db-per-meter="0" />
                <type id="services" db-per-cut="0" db-per-meter="0" />
                
                <type id="shop" db-per-cut="0" db-per-meter="0" />
                <type id="historic" db-per-cut="0" db-per-meter="0" />
                <type id="man_made" db-per-cut="0" db-per-meter="0" />
                <type id="pipeline" db-per-cut="0" db-per-meter="0" />
                <type id="amenity" db-per-cut="0" db-per-meter="0" />
                <type id="parking" db-per-cut="0" db-per-meter="0" />
                <type id="power" db-per-cut="0" db-per-meter="0" />
                <type id="highway" db-per-cut="0" db-per-meter="0" />

                <type id="boundary" db-per-cut="0" db-per-meter="0" />
                <type id="admin_level" db-per-cut="0" db-per-meter="0" />
                	
            </obstacles>
        </AnalogueModel>        
    </AnalogueModels>
    <Decider type="Decider80211p">
        <!-- The center frequency on which the phy listens-->
        <parameter name="centerFrequency" type="double" value="5.890e9"/>
    </Decider>
</root>
